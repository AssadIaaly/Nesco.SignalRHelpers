@using UserManagementAndControl.MudBlazor.Client

<Router AppAssembly="typeof(Program).Assembly" AdditionalAssemblies="new[] { typeof(UserManagementAndControl.MudBlazorExample.Client._Imports).Assembly }">
    <Found Context="routeData">
        <AuthorizeRouteView RouteData="routeData" DefaultLayout="typeof(UserManagementAndControl.MudBlazorExample.Client.Layout.MainLayout)">
            <NotAuthorized>
                <RedirectToLogin/>
            </NotAuthorized>
        </AuthorizeRouteView>
        <FocusOnNavigate RouteData="routeData" Selector="h1"/>
    </Found>
</Router>
